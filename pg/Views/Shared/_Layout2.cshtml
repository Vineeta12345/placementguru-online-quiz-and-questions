<!DOCTYPE html>
<html>
<head>

    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <meta name="viewport" content="width=device-width" />
    <title>Dashboard</title>
</head>
<body>
    <div class="row">
        <div class="col-lg-2 col-md-2 cold-sm-2 col-xs-2" style="height:900px; background-color:#000000; border: 1px solid gray">
            <div style="width:100%;" class="list-group">
                <ul style="color:#f7f5f2; font-family:Chiller; font-size:30px; font-weight:bold">
              <li> @Html.ActionLink("Add Question", "Addquestion",new { @class= "list-group-item" })</li>
               <li>@Html.ActionLink("Add Category", "Addcategory", new { @class = "list-group-item" })</li>
              <li> @Html.ActionLink("Report", "Dashboard", new { @class = "list-group-item" })</li>
              <li> @Html.ActionLink("Logout", "logout", new { @class = "list-group-item" })</li>
                </ul>
               
            </div>

</div>
        <div class="col-lg-10 col-md-10 cold-sm-10 col-xs-10" style="height:900px; border: 1px solid gray">

            @RenderBody()
        </div>
    </div>
</body>
</html>
